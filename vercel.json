{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/create-checkout-session.js" }
  ],
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "src/api/create-checkout-session.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/create-checkout-session.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}