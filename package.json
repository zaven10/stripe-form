{
  "name": "stripe-form",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node .output/server/index.mjs",
    "build": "npm run cleanup && npm run devtools:disable && nuxt build",
    "dev": "nuxt dev -o",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
