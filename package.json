    "path": "^0.12.7",
    "sass": "^1.69.5",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@iconify-json/skill-icons": "^1.2.0",
